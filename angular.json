{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "examen": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/examen",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "library/bootstrap-4.1.0-dist/css/bootstrap.min.css",
              "library/bootstrap-4.1.0-dist/css/bootstrap-grid.min.css",
              "library/bootstrap-4.1.0-dist/css/bootstrap-reboot.min.css",
              "node_modules/noty/lib/noty.css",
              "node_modules/noty/lib/themes/bootstrap-v4.css",
              "node_modules/noty/lib/themes/light.css",
              "node_modules/noty/lib/themes/metroui.css",
              "node_modules/noty/lib/themes/mint.css",
              "node_modules/noty/lib/themes/nest.css",
              "node_modules/noty/lib/themes/relax.css",
              "node_modules/noty/lib/themes/semanticui.css",
              "node_modules/noty/lib/themes/sunset.css",
              "library/animate.min.css",
              "node_modules/leaflet/dist/leaflet.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/popper.js/dist/umd/popper-utils.min.js",
              "library/bootstrap-4.1.0-dist/js/bootstrap.bundle.min.js",
              "library/bootstrap-4.1.0-dist/js/bootstrap.min.js",
              "node_modules/jquery-base64/jquery.base64.min.js",
              "node_modules/noty/lib/noty.min.js",
              "library/anime.min.js",
              "library/socket.io-1.4.5.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "examen:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "examen:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "examen:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "library/bootstrap-4.1.0-dist/css/bootstrap.min.css",
              "library/bootstrap-4.1.0-dist/css/bootstrap-grid.min.css",
              "library/bootstrap-4.1.0-dist/css/bootstrap-reboot.min.css",
              "node_modules/noty/lib/noty.css",
              "node_modules/noty/lib/themes/bootstrap-v4.css",
              "node_modules/noty/lib/themes/light.css",
              "node_modules/noty/lib/themes/metroui.css",
              "node_modules/noty/lib/themes/mint.css",
              "node_modules/noty/lib/themes/nest.css",
              "node_modules/noty/lib/themes/relax.css",
              "node_modules/noty/lib/themes/semanticui.css",
              "node_modules/noty/lib/themes/sunset.css",
              "library/animate.min.css",
              "node_modules/leaflet/dist/leaflet.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/popper.js/dist/umd/popper.min.js",
              "node_modules/popper.js/dist/umd/popper-utils.min.js",
              "library/bootstrap-4.1.0-dist/js/bootstrap.bundle.min.js",
              "library/bootstrap-4.1.0-dist/js/bootstrap.min.js",
              "node_modules/jquery-base64/jquery.base64.min.js",
              "node_modules/noty/lib/noty.min.js",
              "library/anime.min.js",
              "library/socket.io-1.4.5.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "examen:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "examen:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "examen",
  "cli": {
    "analytics": "015d06c3-17a2-4df0-88b5-174297e5fa7e"
  }
}